package 案例分析;
class Account1{
	private String name;
	private double balance;
	public Accoun1(){}
	public Account1(String name){
		this(name, 0.0); // 调用双参构造
	}
	public Account1(String name, double balance){
		this.name = name;
		this.balance = balance;
	}
	
}
public class Account {
	public static void main(String args[]){
		
	}

}
