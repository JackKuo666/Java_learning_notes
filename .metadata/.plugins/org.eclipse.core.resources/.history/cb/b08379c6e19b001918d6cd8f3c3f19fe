package 案例分析;
class Account1{
	private String name;
	private double balance;
	public Account(){}
	public Account(String name){
		this.name = name;
	}
	public Account(String name, double balance){
		this.name = name;
		this.balance = balance;
	}
	
}
public class Account {
	public static void main(String args[]){
		
	}

}
